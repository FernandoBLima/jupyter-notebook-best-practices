    "Exemplo de estrutura completa:\n",
    "\n",
    "\t|- notebooks/\n",
    "\t   |- 00-first-logical-notebook.ipynb\n",
    "\t   |- 01-second-logical-notebook.ipynb\n",
    "\t   |- archive/\n",
    "\t\t  |- no-longer-useful.ipynb\n",
    "       |- figures/\n",
    "          |- image1.png\n",
    "\t|- src/\n",
    "        |- __init__.py\n",
    "        |- config.py\n",
    "        |- data.py\n",
    "        |- utils.py\n",
    "\t|- README.md\n",
    "    |- reports\n",
    "       |- report1.csv\n",
    "       |- report2.pdf\n",
    "\t|- data/\n",
    "\t   |- raw/\n",
    "\t   |- processed/\n",
    "\t   |- cleaned/\n",
    "\t|- utils/\n",
    "\t   |- script1.py\n",
    "\t   |- script2.py\n",
    "\t   |- archive/\n",
    "\t      |- no-longer-useful.py\n",
    "\t|- environment.yml"
