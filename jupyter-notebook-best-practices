    "def split_email(email: str, temp: bool):\n",
    "    if temp:\n",
    "        return email.split('@')[0]\n",
    "    else:\n",
    "        return email.split('@')[1]\n",
    "\n",
    "split_email('ola@tudobom.com', True)"
   ]
